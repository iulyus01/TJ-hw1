<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>NumberToListServlet</servlet-name>
        <servlet-class>com.test.NumberToListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NumberToListServlet</servlet-name>
        <url-pattern>/number2tree</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sum</servlet-name>
        <servlet-class>com.test.Sum</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sum</servlet-name>
        <url-pattern>/sum</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Simple</servlet-name>
        <servlet-class>com.test.Simple</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Simple</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FormPage</servlet-name>
        <servlet-class>com.test.FormPage</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FormPage</servlet-name>
        <url-pattern>/form</url-pattern>
    </servlet-mapping>

